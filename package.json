{
  "name": "vocab-study",
  "version": "1.0.0",
  "description": "An app to study English vocabulary",
  "main": "src",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "npm run webpack-dev-server -- --env mode=development  --hot",
    "dev:debug": "npm run debug",
    "dev:web": "npm run dev -- --env presets=web",
    "debugthis": "node --inspect --inspect-brk ./src/index.js",
    "prod": "npm run webpack  -- --env mode=production",
    "prod:debug": "npm run debug",
    "prod:web": "npm run prod -- --env presets=web",
    "prod:typescript": "npm run prod --env presets=typescript",
    "prod:analyze": "npm run prod --env presets=analyze",
    "prod:compress": "npm run prod --env presets=compress",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "author": "Minh Le",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/styled-components": "^5.1.25",
    "babel-loader": "^8.2.5",
    "compression-webpack-plugin": "^9.2.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "howler": "^2.2.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.5"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
